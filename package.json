{
  "name": "react-css-spinners-tutorial",
  "version": "1.0.0",
  "description": "CSS only spinner for React",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/miguelangeltorresfp/react-css-spinner-tutorial.git"
  },
  "keywords": [
    "rect",
    "css",
    "spinner"
  ],
  "author": "Miguel Angel Torres FP",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miguelangeltorresfp/react-css-spinner-tutorial/issues"
  },
  "homepage": "https://github.com/miguelangeltorresfp/react-css-spinner-tutorial#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-options": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "prettier-eslint \"src/**/*.js\" --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 9001 -c storybook",
    "storybook:build": "build-storybook -c storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/mocks/styles.js"
    }
  }
}
